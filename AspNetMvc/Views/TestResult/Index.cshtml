
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Tests</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <style type='text/css'>
        h3 {
            font-family: sans-serif;
            color: white;
            text-indent: 1.5em;
        }
    </style>
</head>
<body>
    <div class="navbar navbar-inverse">
        <h3>Tests Results</h3>
    </div>

    <div class="row panel">
        @Html.ActionLink("Tests Results (Complete List)", "TestResultsList", null,
             new { @class = "btn btn-block btn-default btn-lg" })
        @Html.ActionLink("Test Results (By Course)", "TestResultsByCourse", null,
             new { @class = "btn btn-block btn-default btn-lg" })
        @Html.ActionLink("Test Results (Selection Form)", "TestResultSelection", null,
             new { @class = "btn btn-block btn-default btn-lg" })
    </div>
</body>
</html>
